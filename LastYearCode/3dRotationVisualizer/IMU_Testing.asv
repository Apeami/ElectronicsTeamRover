% Testing
% Clean up the workbench
clc; close all; clear all; clearvars -global;

f = fopen("/dev/cu.usbmodem14101");
serialportObj = serialport("/dev/cu.usbmodem14101",115200);



% Velocity against time


serialportObj.NumBytesAvailable
flush(serialportObj)
serialportObj.NumBytesAvailable

data1 = read(serialportObj,100,"int");

fclose(f);

velocity = [];
velocity = data1;

time = 1:100;

figure('Name', 'IMU Testing', 'NumberTitle', 'off');

plot(time, velocity,'g')
hold on
title('Velcoity vs Time')
xlabel('Time')
ylabel('Velocity')
hold off

flushinput(serialportObj);
